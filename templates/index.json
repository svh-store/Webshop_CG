\/* top *\/           linear-gradient(var(--c),var(--c)) top    left,\n    \/* left *\/          linear-gradient(var(--c),var(--c)) top    left,\n    \/* right *\/         linear-gradient(var(--c),var(--c)) top    right,\n    \/* bottom-left *\/   linear-gradient(var(--c),var(--c)) bottom left,\n    \/* bottom-right *\/  linear-gradient(var(--c),var(--c)) bottom right;\n  background-repeat:no-repeat;\n  background-size:\n     0% var(--b),      \/* top (Breite) *\/\n     var(--b) 0%,      \/* left (Höhe)  *\/\n     var(--b) 0%,      \/* right (Höhe) *\/\n     0% var(--b),      \/* bottom-left  *\/\n     0% var(--b);      \/* bottom-right *\/\n  border: var(--b) solid transparent; \/* sichtbar wird es durch die Gradients *\/\n  border-radius:4px; \/* Radius für die Optik beibehalten *\/\n}\n\n\/* Hover\/Fokus für Desktop; auf Mobile starten wir via .animate *\/\n@media (hover:hover){\n  .stats-item.border-video:hover,\n  .stats-item.border-video:focus{ animation:border-video-draw var(--t) var(--e) forwards; }\n}\n\n\/* Auto-Start via JS (IntersectionObserver) *\/\n.stats-item.border-video.animate{\n  animation:border-video-draw var(--t) var(--e) forwards;\n}\n\n@keyframes border-video-draw{\n  \/* Phase 1: Top wächst vollständig von links nach rechts *\/\n  0%{\n    background-size:0% var(--b),var(--b) 0%,var(--b) 0%,0% var(--b),0% var(--b);\n  }\n  40%{\n    background-size:100% var(--b),var(--b) 0%,var(--b) 0%,0% var(--b),0% var(--b);\n  }\n  \/* Phase 2: Linke & rechte Linie wachsen gleichzeitig nach unten *\/\n  75%{\n    background-size:100% var(--b),var(--b) 100%,var(--b) 100%,0% var(--b),0% var(--b);\n  }\n  \/* Phase 3: Unten wächst von beiden Seiten zur Mitte *\/\n  100%{\n    background-size:100% var(--b),var(--b) 100%,var(--b) 100%,50% var(--b),50% var(--b);\n  }\n}\n\n\/* Breakpoints *\/\n@media (max-width:1024px){\n  .stats-desktop{display:none}\n  .stats-mobile{display:block}\n  .stats-section .counter{font-size:28px}\n  .stats-section .stats-icon{width:96px}\n  .stats-section .stats-item div:last-child{font-size:14px}\n}\n@media (max-width:640px){\n  .stats-mobile .slides{gap:12px;padding:0 16px}\n  .stats-mobile .stats-item{min-width:86%;max-width:86%}\n  .stats-section .counter{font-size:24px}\n  .stats-section .stats-icon{width:80px}\n  .stats-section .stats-item div:last-child{font-size:13px}\n}\n\n\/* Bewegungsreduktion *\/\n@media (prefers-reduced-motion:reduce){\n  .stats-section .stats-item,\n  .stats-section .stats-icon{transition:none!important}\n  .stats-item.border-video,\n  .stats-item.border-video:hover,\n  .stats-item.border-video:focus{\n    animation:none!important;\n    background-size:100% var(--b),var(--b) 100%,var(--b) 100%,50% var(--b),50% var(--b);\n  }\n}\n<\/style>\n\n<script>\ndocument.addEventListener(\"DOMContentLoaded\",()=>{\n  const section=document.querySelector(\".stats-section\");\n  if(!section) return;\n\n  \/* Counter-Animation *\/\n  const counters=section.querySelectorAll(\".counter\");\n  const animateCounter=c=>{\n    const target=+c.dataset.target;\n    const duration=1500;\n    const start=performance.now();\n    const easeInOutQuad=t=>t<.5?2*t*t:-1+(4-2*t)*t;\n    const step=now=>{\n      const p=Math.min((now-start)\/duration,1);\n      const eased=easeInOutQuad(p);\n      c.textContent=Math.round(target*eased).toLocaleString();\n      if(p<1) requestAnimationFrame(step);\n    };\n    requestAnimationFrame(step);\n  };\n\n  \/* IntersectionObserver: startet Counter + Border-Video einmalig *\/\n  const items=[...section.querySelectorAll(\".stats-item.border-video\")];\n  const io=new IntersectionObserver(entries=>{\n    entries.forEach(e=>{\n      if(e.isIntersecting){\n        const item=e.target;\n        \/* Border-Animation nur einmal triggern *\/\n        if(!item.classList.contains(\"animate\")){\n          item.classList.add(\"animate\");\n        }\n        \/* Den enthaltenen Counter starten (einmalig) *\/\n        const c=item.querySelector(\".counter\");\n    